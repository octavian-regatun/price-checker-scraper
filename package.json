  "version": "1.0.0",
  "description": "price-checker-backend",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon ./src/index.js",
    "start:prod": "node ./src/index.js",
    "test:prod": "npm run build && jest ./build/tests --env=node",
    "test:dev": "jest ./src/tests --env=node",
    "build": "npm run clear && npm run babel-build",
